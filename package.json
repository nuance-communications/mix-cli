{
  "name": "@nuance-mix/mix-cli",
  "description": "Mix Command Line Interface",
  "version": "0.0.0-semantically-released",
  "contributors": [
    "Francois Groleau",
    "Oliver Daniel",
    "Vishal Sangave",
    "Pallavi Pawar"
  ],
  "homepage": "https://github.com/nuance-communications/mix-cli",
  "keywords": [
    "cli",
    "oclif"
  ],
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "bin": {
    "mix": "./bin/run"
  },
  "bugs": "https://community.mix.nuance.com",
  "dependencies": {
    "@oclif/command": "1.8.16",
    "@oclif/config": "1.18.2",
    "@oclif/plugin-autocomplete": "^1.3.0",
    "@oclif/plugin-help": "3.2.17",
    "axios": "0.26.0",
    "chalk": "4.1.2",
    "cli-ux": "5.6.3",
    "date-fns": "2.28.0",
    "debug": "4.3.3",
    "dotenv": "14.3.2",
    "form-data": "4.0.0",
    "simple-oauth2": "4.3.0",
    "tslib": "2.3.1",
    "uuid": "8.3.2",
    "yaml": "1.10.2",
    "zod": "3.11.6"
  },
  "devDependencies": {
    "@commitlint/cli": "16.2.3",
    "@commitlint/config-conventional": "16.2.1",
    "@oclif/dev-cli": "1.26.10",
    "@oclif/test": "1.2.9",
    "@octokit/core": "3.5.1",
    "@semantic-release/changelog": "6.0.1",
    "@semantic-release/git": "10.0.1",
    "@types/chai": "4.3.0",
    "@types/debug": "4.1.7",
    "@types/express": "4.17.13",
    "@types/mocha": "9.1.0",
    "@types/node": "17.0.13",
    "@types/simple-oauth2": "4.1.1",
    "@types/uuid": "8.3.4",
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "chai": "4.3.6",
    "eslint": "8.7.0",
    "eslint-config-oclif": "4.0.0",
    "eslint-config-oclif-typescript": "1.0.2",
    "eslint-plugin-mocha": "10.0.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-unicorn": "40.1.0",
    "execa": "5.1.1",
    "globby": "11.0.4",
    "husky": "7.0.4",
    "mocha": "9.2.0",
    "nock": "13.2.2",
    "nyc": "15.1.0",
    "pkg": "5.5.2",
    "semantic-release": "19.0.2",
    "sinon": "12.0.1",
    "sinon-chai": "3.7.0",
    "strip-ansi": "6.0.1",
    "ts-node": "10.4.0",
    "typedoc": "0.22.13",
    "typescript": "4.5.5"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "mocha": {
    "require": [
      "ts-node/register"
    ],
    "watch-extension": "ts",
    "recursive": true,
    "reporter": "spec",
    "file": "test/setup.ts",
    "timeout": "30000"
  },
  "nyc": {
    "extension": [
      ".ts",
      ".tsx"
    ],
    "exclude": [
      "**/*.d.ts",
      "lib/**/*.*",
      "release/**/*.*",
      "test/**/*.*",
      "tmp/**/*.*"
    ],
    "reporter": [
      "html",
      "text"
    ],
    "all": true
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "mix",
    "macos": {
      "identifier": "com.nuance.mix.cli"
    },
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete"
    ],
    "topics": {
      "applications": {
        "description": "manage applications"
      },
      "app-credentials": {
        "description": "manage application credentials"
      },
      "app-configs": {
        "description": "manage application configurations"
      },
      "builds": {
        "description": "manage builds"
      },
      "channels": {
        "description": "manage channels"
      },
      "data-hosts": {
        "description": "manage data hosts"
      },
      "deployment-flows": {
        "description": "manage deployment flows"
      },
      "intents": {
        "description": "manage intents"
      },
      "jobs": {
        "description": "manage jobs"
      },
      "literals": {
        "description": "manage entity literals"
      },
      "projects": {
        "description": "manage projects"
      },
      "samples": {
        "description": "manage samples"
      },
      "ontology": {
        "description": "manage ontology"
      },
      "organizations": {
        "description": "manage organizations"
      },
      "language-topics": {
        "description": "manage language topics"
      },
      "environments": {
        "description": "manage environments"
      }
    }
  },
  "pkg": {
    "scripts": "./lib/**/*.js",
    "assets": [
      "./node_modules/open/xdg-open"
    ]
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "url": "nuance-communications/mix-cli"
  },
  "scripts": {
    "doc:api": "typedoc",
    "eslint": "eslint . --ext .ts --config .eslintrc",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "test:grep": "mocha --forbid-only \"test/**/*.test.ts\" --grep ",
    "transpile": "rm -rf lib && tsc -b",
    "update:readme": "oclif-dev readme",
    "version": "oclif-dev readme && git add README.md",
    "prepare": "husky install"
  },
  "types": "lib/index.d.ts"
}
